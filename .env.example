# TrakSense Frontend - Environment Variables Example
# Copy this file to .env and adjust values as needed

# API Backend URL
VITE_API_URL=http://umc.localhost:8000/api

# App Configuration
VITE_APP_NAME=TrakSense
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_DEMO_MODE=true
VITE_ENABLE_ANALYTICS=false

# Email Service (Resend)
# Required only if email functionality is enabled
# Get your API key from: https://resend.com/api-keys
VITE_RESEND_API_KEY=re_123456789_your_api_key_here

# Support Contact
# Email shown in support/contact forms
VITE_SUPPORT_EMAIL=contato@traksense.io
